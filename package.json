{
  "name": "ts-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^18"
  },
  "engineStrict": true,
  "scripts": {
    "prepare": "echo 'installing husky...' && husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "test": "vitest",
    "preview": "vite preview",
    "lint": "eslint --ext src/**/*.*.{ts,tsx}",
    "prettify": "prettier --config .prettierrc '**/*.{html,css,js,jsx,json,ts,tsx}'",
    "prettify:fix": "prettier --config .prettierrc '**/*.{html,css,js,jsx,json,ts,tsx}' --write",
    "prettify:watch": "onchange '**/*.{html,css,js,jsx,json,ts,tsx}' -- prettier --config .prettierrc --write {{changed}}"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahanahmed86/ts-react.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typescript",
    "ts"
  ],
  "author": "Shahan Ahmed Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shahanahmed86/ts-react/issues"
  },
  "homepage": "https://github.com/shahanahmed86/ts-react#readme",
  "dependencies": {
    "bootstrap": "^5.3.0",
    "husky": "^8.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "gh-pages": "^5.0.0",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "onchange": "^7.1.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^0.31.1"
  }
}
